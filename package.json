{
  "name": "@ocxp/client",
  "version": "0.2.6",
  "description": "TypeScript client for OCXP protocol - auto-generated from OpenAPI spec",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "openapi-ts",
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "keywords": [
    "ocxp",
    "client",
    "typescript",
    "openapi"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OCXP/client-ts.git"
  },
  "homepage": "https://github.com/OCXP/client-ts#readme",
  "bugs": {
    "url": "https://github.com/OCXP/client-ts/issues"
  },
  "dependencies": {
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@hey-api/client-fetch": "^0.13.1",
    "@hey-api/openapi-ts": "^0.89.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "msw": "^2.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^2.0.0"
  }
}
