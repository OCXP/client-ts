{
  "name": "@ocxp/client",
  "version": "0.2.0",
  "description": "TypeScript client for OCXP protocol - auto-generated from OpenAPI spec",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "openapi-ts",
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ocxp",
    "client",
    "typescript",
    "openapi"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@hey-api/client-fetch": "^0.13.1",
    "@hey-api/openapi-ts": "^0.89.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
